@model IEnumerable<Employee_Record.Models.EmpRecord>
@using Employee_Record.Models
@if (Model != null)
{
    <div class="grid">
        <table cellspacing="0" width="80%">
            <thead>
                <tr>
                    <th>
                        Id
                    </th>
                    <th>
                        Name
                    </th>
                    <th>
                        City
                    </th>
                    

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td align="left">
                            @item.Id
                        </td>
                        <td align="left">
                            @item.Name
                        </td>
                        <td align="left">
                            @item.City
                        </td>
                       
                    </tr>
                }
            </tbody>
        </table>

    </div>



}